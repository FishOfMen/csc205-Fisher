
@{
    ViewBag.Title = "People";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>People</h2>
<ul>
    @foreach (var People in Model)
    {
        <li><a href="/People/Profile/@People.id">@People.firstname @People.middlename @People.lastname</a> - @People.relationship -- <a href="/People/Edit/@People.id">Edit</a></li>
    }
</ul>
